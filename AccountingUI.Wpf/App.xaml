<prism:PrismApplication
             x:Class="AccountingUI.WPF.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:AccountingUI.WPF"
             xmlns:prism="http://prismlibrary.com/"
             DispatcherUnhandledException="PrismApplication_DispatcherUnhandledException">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="pack://application:,,,/Styles/PrintStyle.xaml"/>
                <ResourceDictionary Source="pack://application:,,,/Styles/ButtonStyle.xaml"/>
                <ResourceDictionary Source="pack://application:,,,/Styles/DataGridStyle.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</prism:PrismApplication>
